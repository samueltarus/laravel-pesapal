{
    "name": "samueltarus/laravel-pesapal",
    "description": "A Laravel package for PesaPal integration",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "samueltarus",
            "email": "samuelcheboitarus@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.3|^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^8.0|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "samueltarus\\LaravelPesaPal\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "samueltarus\\LaravelPesaPal\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "samueltarus\\LaravelPesaPal\\PesaPalServiceProvider"
            ],
            "aliases": {
                "PesaPal": "samueltarus\\LaravelPesaPal\\Facades\\PesaPal"
            }
        }
    }
}